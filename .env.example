# Supabase — get these from your project's API settings page
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Supabase service role key — server-side only, never exposed to browser
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Shared secret for Google Forms intake webhook
# Must match the value set in your Google Apps Script
INTAKE_SHARED_SECRET=a-random-secret-string

# Admin token for protected API routes (e.g. POST /api/tenant/:id/cycle/start)
DASHBOARD_ADMIN_TOKEN=a-random-admin-token
